{
    "name": "mitsuha-snapshot",
    "version": "2.1.1-dev.1",
    "main": "dist/Mitsuha.js",
    "imports": {
        "#lib/*": "./dist/lib/*",
        "#root/*": "./dist/*",
        "#builders/*": "./dist/lib/builders/*",
        "#handlers/*": "./dist/lib/handlers/*",
        "#utils/*": "./dist/lib/utils/*"
    },
    "scripts": {
        "build": "tsc -b .",
        "watch": "tsc -b . -W",
        "lint": "yarn lint:pretty && yarn lint:eslint",
        "lint:pretty": "prettier . -w",
        "lint:eslint": "eslint --fix src --ext ts",
        "start": "clear; nodemon --unhandled-rejections=strict --es-module-specifier-resolution=node .",
        "refresh": "rm -rf dist && yarn build"
    },
    "repository": "https://github.com/ayannw/mitsuha-snapshot.git",
    "author": "ayannw <ayanispro420@gmail.com>",
    "license": "MIT",
    "type": "module",
    "engines": {
        "node": ">=14"
    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "@types/glob": "^7.1.3",
        "@types/node": "^14.14.37",
        "@types/node-fetch": "^2.5.9",
        "@types/ws": "^7.4.0",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
        "@typescript-eslint/parser": "^4.21.0",
        "eslint": "^7.23.0",
        "prettier": "^2.2.1",
        "typescript": "^4.2.3"
    },
    "dependencies": {
        "@sapphire/discord.js-utilities": "^1.5.4",
        "@sapphire/stopwatch": "^1.0.6",
        "@sapphire/time-utilities": "^1.2.12",
        "@sapphire/utilities": "^1.4.6",
        "colorette": "^1.2.2",
        "discord.js": "^12.5.3",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "fastest-levenshtein": "^1.0.12",
        "glob": "^7.1.6",
        "node-fetch": "^2.6.1"
    }
}
